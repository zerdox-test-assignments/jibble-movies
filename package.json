{
  "name": "jibble-movies",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines" : {
    "npm" : "^11",
    "node" : "^22 || ^20"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "run-p test:unit test:e2e",
    "test:unit": "vitest",
    "test:e2e": "npm run cy:run",
    "cy:open": "cypress open --config-file cypress.config.mts",
    "cy:run": "cypress run --config-file cypress.config.mts"
  },
  "dependencies": {
    "@vueuse/core": "12.5.0",
    "ky": "1.7.4",
    "pinia": "2.3.1",
    "pinia-plugin-persistedstate": "4.2.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vuetify": "3.7.7"
  },
  "devDependencies": {
    "@mdi/font": "7.4.47",
    "@ngneat/falso": "7.3.0",
    "@pinia/testing": "0.1.7",
    "@tsconfig/node22": "22.0.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.10.7",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "3.0.3",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.3.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "cypress": "14.0.0",
    "eslint": "9.18.0",
    "eslint-plugin-vue": "9.32.0",
    "jiti": "2.4.2",
    "jsdom": "26.0.0",
    "nanoid": "5.0.9",
    "npm-run-all2": "7.0.2",
    "prettier": "3.4.2",
    "sass-embedded": "1.83.4",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vite-plugin-vue-devtools": "7.7.0",
    "vite-plugin-vuetify": "2.0.4",
    "vitest": "3.0.3",
    "vue-tsc": "2.2.0"
  }
}
